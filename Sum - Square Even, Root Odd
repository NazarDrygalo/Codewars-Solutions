#https://www.codewars.com/kata/5a4b16435f08299c7000274f/train/python


def sum_square_even_root_odd(nums):
    sum = 0
    for x in nums:
        if x % 2 == 0:
            evenResult = x**2
            sum += evenResult
        else:
            oddResult = x**0.5
            sum += oddResult
    return round(sum, 2)
